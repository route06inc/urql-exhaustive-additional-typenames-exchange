{
  "name": "urql-exhaustive-additional-typenames-exchange",
  "description": "urql exchange that add all list fields in an operation to additionalTypenames",
  "version": "0.1.2",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/route06inc/urql-exhaustive-additional-typenames-exchange.git"
  },
  "keywords": ["urql", "exchange", "graphql", "exchanges"],
  "files": ["LICENSE", "CHANGELOG.md", "README.md", "dist/"],
  "bugs": {
    "url": "https://github.com/route06inc/urql-exhaustive-additional-typenames-exchange/issues"
  },
  "homepage": "https://github.com/route06inc/urql-exhaustive-additional-typenames-exchange",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "setup": "corepack enable pnpm",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest",
    "lint": "biome lint .",
    "format": "biome check --apply ."
  },
  "dependencies": {
    "@urql/core": ">=4.0.0",
    "@urql/exchange-context": ">=0.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@types/node": "20.11.7",
    "tsup": "8.0.2",
    "typescript": "5.4.2",
    "vitest": "1.3.1"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "packageManager": "pnpm@8.15.4"
}
